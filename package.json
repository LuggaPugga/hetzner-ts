{
  "name": "hetzner-ts",
  "module": "index.ts",
  "type": "module",
  "version": "1.0.3",
  "private": false,
  "keywords": [
    "hetzner",
    "hcloud",
    "typescript",
    "sdk",
    "api"
  ],
  "description": "A TypeScript SDK for the Hetzner Cloud API",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
