    "format": "prettier --write .",
    "docs": "typedoc --out docs",
    "typecheck": "tsc --noEmit",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.4",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
